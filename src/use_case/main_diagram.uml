@startuml
  actor Експерт
  actor Менеджер
  actor Замовник
  actor Аналітик
  
  usecase "Надсилання завдання" as CS_1
  usecase "Взаємодія з Замовником" as MG_1
  usecase "Взаємодія з Аналітиком" as MG_2
  usecase "Взаємодія з Експертом" as MG_3
  usecase "Проходження опитувань" as EX_1
  usecase "Аналіз опитування" as AL_1
  usecase "Створення опитування" as AL_2
  
  
  Замовник -r-|> Менеджер
  Менеджер -u-|> Аналітик
  Менеджер -u-|> Експерт
  

  Замовник -u-> CS_1
  Менеджер -r-> MG_1
  Менеджер -d-> MG_2
  Менеджер -d-> MG_3
  Експерт -u-> EX_1
  Аналітик -u-> AL_1
  Аналітик -r-> AL_2

  

@enduml
